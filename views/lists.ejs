<body>
<h1>Current Users</h1>
<% users.forEach(user=>{ %>
    <div class="user">
        <h3> First Name: <%= user.firstName %>
        </h3>
        <h3> Last Name: <%= user.lastName %>
        </h3>
        <h3> Gender: <%= user.gender %>
        </h3>
        <h3> Age: <%= user.age %>
        </h3>
    </div>
<% }); %>
</body>

</html>
